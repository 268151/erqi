  <update id="updateScore">
  update t_mapping_unit
  <if test="scorePlus != null">
    set score = score + #{scorePlus}
  </if>
  <if test="scoreSubtract != null" >
    set score = score- #{scoreSubtract}
  </if>
  <where>
  id = #{unitId}
  </where>
  </update>
  <select id="selectUpdateAuditStatus" resultType="com.aaa.model.T_mapping_unit">
    select * from t_mapping_unit where audit_status=2
    <if test="unit_name != null">
      and unit_name=#{unit_name}
    </if>
  </select>
  <select id="selectRegistAuditStatus" resultType="com.aaa.model.T_mapping_unit">
    select * from t_mapping_unit where audit_status=3
    <if test="unit_name != null">
      and unit_name=#{unit_name}
    </if>
  </select>



  <select id="selectByWhite" resultMap="BaseResultMap">
  SELECT * FROM t_mapping_unit where score>100
  </select>
  <select id="selectByBlack" resultMap="BaseResultMap">
  SELECT * FROM t_mapping_unit where  60 > score
  </select>
  <select id="getRomdan" resultMap="BaseResultMap">
    SELECT * FROM t_mapping_unit
    where qualification_level='丙级' or qualification_level='丁级'
    <if test="qu != null">
      and owned_district=#{qu}
    </if>
  </select>



